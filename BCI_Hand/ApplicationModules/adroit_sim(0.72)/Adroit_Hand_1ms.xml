<mujoco model="Adroit_hand">
	<compiler 	uniquevertex="false" 
				inertiafromgeom="true" 
				settotalmass="15" 
				meshdir ="Meshes\"
				strippath="false"
				coordinate="local" 
				angle="radian"
				/>
	
	<size 	njmax="500"
			nlmax="500"
			njnt_user="1"
			nsite_user="1"
			nstack="6000000"/>
			
	<option	timestep="0.001"
			s_compliance ="1 1"
			integrator="implicit"
			viscosity="0"
			iterations="20"
			collisionmode="pair">
			<disable clampvel="true"/>
	</option>

	<default>
		<joint islimited="true" makeactuator="false"  damping="0.005" armature=".001"  mindist="0.01" maxvel="10" timeconst="0.01"/>
		
		<geom defset="0" friction="1 0.1 0.1" rgba="0.7 0.7 0 1" mindist="0.001" compliance="0.0001" contype="0" conaffinity="0"/>
		<geom defset="1" friction="1 0.1 0.1" rgba=".4 .5 .6 .5" mindist="0.001" compliance="0.0001" timeconst="0.01" contype="1" conaffinity="0" group="0"/> <!--collission geoms-->
		<geom defset="2" rgba="0.8 0.2 0.1 0.4" group="2" contype="0" conaffinity="0"/> <!--wrap geoms-->
		
		<!--Meshes-->
		<geom defset="3" group="1" contype="0" conaffinity="0" mindist="0.001" rgba="1 0.5 0 1"/>
		<mesh defset="3" scale="0.001 0.001 0.001"/>	
		
		<tendon makeactuator="false" islimited="true" timeconst="0.01"/>
		
		<!--Joint Motors --> 
		<!--<act-general forcerange="-1 1" dyntype="none" trntype="joint" gaintype="fixed" dynprm="0 0" trnprm="1" gainprm="1 0" />-->
		
		<!--Tendon Motors--> 
		<act-general isforcelimited="true" trntype="tendon" forcerange="-37.2 0" gainprm="-1" />
		
		<!--Pneumatic tendons--> 
		<!--<actuator dyntype="none" range="0 10" trntype="tendon" gaintype="fixed" gainprm="600 0" dynprm="101325.0000   500000.0000 0.7807 0.6885 8.2899 -0.9869 -5.8201 20.1557 0.4177 0.0100"/>-->
	</default>
		
	<contactpair defaultstate="true">
		<!--Distals with Distals-->
		<enable geom1="C_ffdistal" geom2="C_thdistal" condim="1"/>	
		<enable geom1="C_mfdistal" geom2="C_thdistal" condim="1"/>	
		<enable geom1="C_rfdistal" geom2="C_thdistal" condim="1"/>	
		<enable geom1="C_lfdistal" geom2="C_thdistal" condim="1"/>	
		<enable geom1="C_mfdistal" geom2="C_ffdistal" condim="1"/>	
		<enable geom1="C_rfdistal" geom2="C_mfdistal" condim="1"/>	
		<enable geom1="C_lfdistal" geom2="C_rfdistal" condim="1"/>	

		<!--Proximals with Proximals-->
		<enable geom1="C_mfproximal" geom2="C_ffproximal" condim="1"/>	
		<enable geom1="C_rfproximal" geom2="C_mfproximal" condim="1"/>	
		<enable geom1="C_lfproximal" geom2="C_rfproximal" condim="1"/>	
		
		<!--little finger -->
		<enable geom1="C_lfdistal" geom2="C_rfmiddle" condim="1"/>	
		<enable geom1="C_lfdistal" geom2="C_mfmiddle" condim="1"/>	
		<enable geom1="C_lfmiddle" geom2="C_rfmiddle" condim="1"/>
	</contactpair>
	
